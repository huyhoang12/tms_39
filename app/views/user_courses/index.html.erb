<%= will_paginate %>
<ul class="users">
  <h1><%= t "member"%></h1>
  <div class="row">
    <% @user_courses.each do |course| %>
      <div class="col-md-12">
        <%= link_to Course.find_by(id: course.course_id).name,
          Course.find_by(id: course.course_id) %>
        <%= t "thuoc"%>
        <%= link_to User.find_by(id: course.user_id).name,
          User.find_by(id: course.user_id) %>
      </div>
    <% end %>
  </div>
</ul>
<%= will_paginate %>
